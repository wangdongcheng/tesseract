    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/api/.libs/tesseract",
            "args": ["--list-langs"],
                    // ["/home/tonywang/Pictures/ocr_test_pic/sFPWe.png"],
                    //  "/home/tonywang/Pictures/ocr_test_pic/sFPWe"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/src/api/.libs/",
            "environment": [],
            "externalConsole": false,
            "preLaunchTask": "build",
            "MIMode": "gdb",
            "logging": {
                "programOutput": true
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}